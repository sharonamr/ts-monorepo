
{
  "extends": "../../base.tsconfig.json",
  "include": [
    "src",
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@ts-monorepo/data/*": [
        "../data/src/*"
      ]
    },
    "target": "es6",
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx"
  },
}
